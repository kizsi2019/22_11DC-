<Window x:Class="totoGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:totoGUI"
        mc:Ignorable="d"
        Title="Totó eredmény ellenőrző" Height="306" Width="546">
    <Grid>
        <Label Content="Kérem a forduló eredményeit [1, 2, X]" HorizontalAlignment="Left" Height="33" Margin="10,35,0,0" VerticalAlignment="Top" Width="390"/>
        <CheckBox Name="cbxHossz" Content="Nem megfelelő a karakterek száma (14)" HorizontalAlignment="Left" Height="34" Margin="16,135,0,0" VerticalAlignment="Top" Width="357" IsEnabled="False"/>
        <CheckBox Name="cbxKarakter" Content="Helytelen karakterek az eredményekben ()" HorizontalAlignment="Left" Height="43" Margin="16,174,0,0" VerticalAlignment="Top" Width="378" IsEnabled="False"/>
        <Button Name="btnMentes" Content="Eredmények mentése" HorizontalAlignment="Left" Height="42" Margin="16,222,0,0" VerticalAlignment="Top" Width="284"/>
        <TextBox Name="txbInput" HorizontalAlignment="Left" Height="37" Margin="16,73,0,0" TextWrapping="Wrap" Text="12X12X12X12X12X" VerticalAlignment="Top" Width="378" FontSize="15" TextChanged="txbInput_TextChanged"/>

    </Grid>
</Window>
